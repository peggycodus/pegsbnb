<%= form_for @property do |f| %>

  <div class="form-group">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="form-group">
    <%= f.label :address %><br />
    <%= f.text_field :address %>
  </div>
  <div class="form-group">
    <%= f.label :city %><br />
    <%= f.text_field :city %>
  </div>
  <div class="form-group">
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </div>
  <div class="form-group">
    <%= f.label :country %><br />
    <%= f.text_field :country %>
  </div>
  <div class="form-group">
    <%= f.label :postal_code %><br />
    <%= f.text_field :postal_code %>
  </div>
  <div class="form-group">
    <%= f.label :min_stay %><br />
    <%= f.text_field :min_stay %>
  </div>
  <div class="form-group">
    <%= f.label :num_beds %><br />
    <%= f.text_field :num_beds %>
  </div>
 <div class="form-group">
    <%= f.label :max_guests %><br />
    <%= f.text_field :max_guests %>
  </div>
  <div class="form-group">
    <%= f.label :price %><br />
    <%= f.text_field :price %>
  </div>
  <p><%= f.submit "Sign Up", class: "btn btn-success"%></p>
<% end %>
